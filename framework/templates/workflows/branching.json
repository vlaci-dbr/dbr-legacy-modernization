        "conversation_config": {
          "tts": { "voice_id": "ORCHESTRATOR_VOICE_ID" },
          "agent": {
            "first_message": "ORCHESTRATOR_GREETING",
            "prompt": {
              "prompt": "ORCHESTRATOR_PROMPT",
              "llm": "gpt-4o-mini",
              "temperature": 0.0
            }
          }
        },
        "additional_prompt": "",
        "additional_knowledge_base": [],
        "additional_tool_ids": []
      },
      "node_specialist_1": {
        "type": "override_agent",
        "label": "SPECIALIST_1_NAME",
        "position": { "x": 200.0, "y": 450.0 },
        "edge_order": ["edge_specialist_1_to_finalizer"],
        "conversation_config": {
          "tts": { "voice_id": "SPECIALIST_1_VOICE_ID" },
          "agent": {
            "prompt": {
              "prompt": "SPECIALIST_1_PROMPT",
              "llm": "gpt-4o",
              "temperature": 0.2
            }
          }
        },
        "additional_prompt": "",
        "additional_knowledge_base": [],
        "additional_tool_ids": []
