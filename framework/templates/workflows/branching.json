        "label": "FINALIZER_NAME",
        "position": { "x": 400.0, "y": 700.0 },
        "edge_order": [],
        "conversation_config": {
          "tts": { "voice_id": "FINALIZER_VOICE_ID" },
          "agent": {
            "prompt": {
              "prompt": "FINALIZER_PROMPT",
              "llm": "gpt-4o",
              "temperature": 0.0
            }
          }
        },
        "additional_prompt": "",
        "additional_knowledge_base": [],
        "additional_tool_ids": []
      }
    },
    "edges": {
      "edge_start_to_orchestrator": {
        "source": "start_node",
        "target": "node_orchestrator",
        "forward_condition": { "type": "unconditional", "label": null },
        "backward_condition": null
      },
      "edge_orchestrator_to_specialist_1": {
        "source": "node_orchestrator",
        "target": "node_specialist_1",
        "forward_condition": {
          "type": "llm",
          "label": null,
