        "edge_order": ["edge_specialist_2_to_finalizer"],
        "conversation_config": {
          "tts": { "voice_id": "SPECIALIST_2_VOICE_ID" },
          "agent": {
            "prompt": {
              "prompt": "SPECIALIST_2_PROMPT",
              "llm": "gpt-4o",
              "temperature": 0.2
